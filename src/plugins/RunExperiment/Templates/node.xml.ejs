<?xml version="1.0"?>
<node name="<%- nodeInfo.name %>">
  <priority setting="<%- nodeInfo.Priority %>"/>
<% 
if (nodeInfo.Component_list) {
nodeInfo.Component_list.map(function(comp) {
-%>
  <component_instance name="<%- comp.name %>">
    <library name="lib<%- comp.base.name %>.so"/>
    <scheduling_scheme setting="<%- comp.SchedulingScheme %>"/>
    <numCompsToSync setting="1"/>
    <syncTimeout setting="0.0"/>
    <logging>
      <is_periodic_logging setting="<%- comp.EnableLogging ? "True" : "False" %>"/>
      <periodic_log_unit setting="<%- comp.LoggingUnit %>"/>
    </logging>    
  </component_instance>
<% 
});
}
-%>
</node>
