<?xml version="1.0"?>
<node name="<%- nodeInfo.name %>">
  <priority setting="<%- nodeInfo.Priority %>"/>
<% 
if (nodeInfo['Component Instance_list']) {
nodeInfo['Component Instance_list'].map(function(ci) {
-%>
  <component_instance name="<%- ci.name %>">
    <library name="lib<%- ci.Component.name %>.so"/>
    <scheduling_scheme setting="<%- ci.SchedulingScheme %>"/>
    <numCompsToSync setting="1"/>
    <syncTimeout setting="0.0"/>
    <logging>
      <is_periodic_logging setting="<%- ci.EnableLogging ? "True" : "False" %>"/>
      <periodic_log_unit setting="<%- ci.LoggingUnit %>"/>
    </logging>    
  </component_instance>
<% 
});
}
-%>
</node>
