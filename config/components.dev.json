{
    "CodeEditor": {
	"theme": "default",
	"enableThemeSelection": true,
	"keyBinding": "sublime",
	"enableKeybindingSelection": true,
	"lineWrapping": false,
	"enableLineWrappingToggle": true,
        "autoSaveInterval": "2000",
	"rootTypes": [
            "Project"
        ],
	"excludeTypes": [
            "Projects",
	    "Experiments",
	    "Systems",
	    "Pseudo State",
            "External Transition",
	    "Initial",
	    "End State"
	],
	"loadDepth": -1,
	"syntaxToModeMap": {
	    "C++": { "name": "text/x-c++src", "useCPP": true },
	    "JSON": { "name": "application/json" },
	    "Javascript": {"name":"text/javascript"},
	    "gfm": { "name": "gfm" },
	    "ROS": { 
		"name":"text/x-c++src", 
		"keywords": {
		    "int8": "int8",
		    "int16": "int16",
		    "int32": "int32",
		    "int64": "int64",
		    "uint8": "uint8",
		    "uint16": "uint16",
		    "uint32": "uint32",
		    "uint64": "uint64",
		    "bool": "bool",
		    "string": "string",
		    "float32": "float32",
		    "float64": "float64",
		    "time": "time",
		    "duration": "duration"
		},
		"useCPP":true
	    }
	},
	"attrToSyntaxMap": {
