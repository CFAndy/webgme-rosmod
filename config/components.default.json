	    },
	    "Component": {
		"Forwards": "C++",
		"Members": "C++",
		"Definitions": "C++",
		"Initialization": "C++",
		"Destruction": "C++",
		"User Configuration": "JSON",
		"User Artifacts": "JSON",
		"Authors": "gfm",
		"Brief Description": "gfm",
		"Detailed Description": "gfm"
	    },
	    "Timer": {
