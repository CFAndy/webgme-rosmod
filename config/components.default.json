{
    "CodeEditor": {
	"theme": "default",
	"enableThemeSelection": true,
	"keyBinding": "sublime",
	"enableKeybindingSelection": true,
	"lineWrapping": false,
	"enableLineWrappingToggle": true,
        "autoSaveInterval": "2000",
	"rootTypes": [
            "Project"
        ],
	"excludeTypes": [
            "Projects",
	    "Experiments",
	    "Systems"
	],
	"loadDepth": 5,
